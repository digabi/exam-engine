{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier", "tslint-plugin-prettier"],
  "jsRules": {},
  "rules": {
    "prettier": true,
    "interface-name": false,
    "jsx-boolean-value": ["never"],
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "member-access": false
  },
  "rulesDirectory": [],
  "linterOptions": { "exclude": ["**/*.js", "**/dist"] }
}

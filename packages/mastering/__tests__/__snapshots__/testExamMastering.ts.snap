            <th>Attribute</th>
            <th>Type</th>
            <th>Mandatory</th>
            <th>Default value</th>
            <th>Description</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td>max-score</td>
            <td>Integer</td>
            <td>No</td>
            <td>0</td>
            <td>Maximum score</td>
          </tr>
          <tr>
            <td>class</td>
            <td>CSS class</td>
            <td>No</td>
            <td>-</td>
            <td>CSS class name</td>
          </tr>
        </tbody>
      </table>

